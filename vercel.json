{
  "version": 2,
  "builds": [
    {
      "src": "packages/ui/package.json",
      "use": "@vercel/static-build",
      "workPath": "packages/ui"
    },
    {
      "src": "apps/web/next.config.js",
      "use": "@vercel/next",
      "config": {
        "distDir": "../../.next"
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/_next/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/web"
    }
  ]
}
